{
  "name": "@youwol/kepler",
  "version": "0.0.8-next",
  "description": "Scientific visualization based on three.js",
  "author": "fmaerten@youwol.com",
  "keywords": [],
  "main": "dist/@youwol/kepler.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "yarn build:dev",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "test": "jest -c ./jest.config.js --rootDir .",
    "test-coverage": "jest -c ./jest.config.js --rootDir . --collect-coverage --collectCoverageFrom=./src/lib/**/*.ts ",
    "doc": "rm -fr  dist/docs && typedoc src/ --exclude src/lib --exclude src/tests --disableSources --out dist/docs --includeVersion --hideGenerator --theme pages-plugin --media ./media",
    "serve": "http-server ../../../../../ -p 80 -o projects/youwol/workspace/packages/kepler/test",
    "upgrade-youwol": "yarn upgrade @youwol/math @youwol/attribute @youwol/dataframe @youwol/io",
    "publish-npm-public": "yarn build:prod && yarn test-coverage && yarn publish --access public"
  },
  "dependencies": {},
  "peerDependencies": {
    "@youwol/dataframe": "0.x",
    "@youwol/three-extra": "0.x",
    "three": "^0.128.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/three": "^0.128.0",
    "@youwol/dataframe": "0.x",
    "@youwol/io": "0.0.5",
    "@youwol/math": "0.x",
    "@youwol/three-extra": "^0.x",
    "eslint": "7.23.0",
    "eslint-webpack-plugin": "2.5.3",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "three": "^0.128.0",
    "ts-jest": "26.5.4",
    "ts-lib": "0.0.5",
    "ts-loader": "8.1.0",
    "tslib": "^2.2.0",
    "typedoc": "0.20.34",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript": "4.2.3",
    "webpack": "5.28.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "4.6.0"
  },
  "youwol": {
    "pipeline": {
      "name": "yw_pipeline_webpack_ts",
      "version": "0.0.0"
    },
    "cdnDependencies": {
      "@youwol/dataframe": "0.x",
      "@youwol/three-extra": "0.x",
      "three": "^0.128.0"
    }
  }
}
